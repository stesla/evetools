{
  "name": "evetools",
  "version": "1.0.0",
  "description": "",
  "main": "sass/styles.scss",
  "scripts": {
    "build:css": "tailwindcss build css/styles.css -o public/css/styles.css",
    "serveui:logged-in": "live-server public --no-browser --port 8080 --middleware=../../fixture/middleware.js --mount=/api/v1:fixture",
    "serveui:logged-out": "live-server public --no-browser --port 8080",
    "start": "modd",
    "watch": "npm-watch"
  },
  "watch": {
    "build:css": {
      "patterns": [
        "css"
      ],
      "extensions": "css"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stesla/evetools.git"
  },
  "author": "Sam Tesla <samuel.tesla@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stesla/evetools/issues"
  },
  "homepage": "https://github.com/stesla/evetools#readme",
  "devDependencies": {
    "alpinejs": "^2.7.3",
    "live-server": "^1.2.1",
    "npm-watch": "^0.7.0",
    "tailwindcss": "^1.9.6"
  }
}
